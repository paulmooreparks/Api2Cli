    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Run a2c",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/a2c/bin/Debug/net8.0/a2c.dll",
            "args": [
                "--config",
                "${workspaceFolder}/ParksComputing.Api2Cli.Tests/TestConfigs/tests.xfer"
            ],
            "cwd": "${workspaceFolder}/a2c",
            "stopAtEntry": false,
            "console": "externalTerminal"
        }
    ]
}
